{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "r2",
  "main": "src/index.ts",
  "compatibility_date": "2024-11-11",
  "observability": {
    "enabled": true
  },
  "r2_buckets": [
    {
      "binding": "R2_BUCKET",
      "bucket_name": "__your_r2_name__"
    }
  ],
  "vars": {
    "AUTH_API_KEY": "__your_backend_key__"
  }
}